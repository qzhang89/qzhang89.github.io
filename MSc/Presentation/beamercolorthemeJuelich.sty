\mode<presentation>

\definecolor{fzjblue50}{RGB}{0,91,130}
\definecolor{fzjblue35}{cmyk}{1,0.2,0.05,0.35}
\definecolor{fzjblue30}{cmyk}{1,0.2,0.05,0.30}
\definecolor{fzjblue20}{cmyk}{1,0.2,0.05,0.20}
\definecolor{fzjblue10}{cmyk}{1,0.2,0.05,0.10}

\definecolor{fzjgray80}{RGB}{81,81,81}
\definecolor{fzjgray50}{RGB}{156,156,156}
\definecolor{fzjgray30}{RGB}{185,185,185}
\definecolor{fzjgray20}{RGB}{204,204,204} % unofficial extension (inkscape conversion from cmyk!)
\definecolor{fzjgray10}{RGB}{229,229,229} % unofficial extension (inkscape conversion from cmyk!)
\definecolor{fzjgray05}{RGB}{242,242,242} % unofficial extension (inkscape conversion from cmyk!)
\definecolor{fzjwhite}{RGB}{255,255,255}

\definecolor{fzjbranchred}{RGB}{212,45,18}% Gesundheit
\definecolor{fzjbranchyellow}{RGB}{230,175,17}% Energie und Umwelt
\definecolor{fzjbranchblue}{RGB}{0,131,190}% Informationstechnologie

\definecolor{fzjblue}{RGB}{0,91,130}% Diagram color blue % 100%
\definecolor{fzjlightblue}{cmyk}{0.42,0.16,0.00,0.26}% Diagram color blue % 50%
\definecolor{fzjred}{RGB}{175,90,80}% Diagram color red % 100%
\definecolor{fzjlightred}{cmyk}{0.0,0.3,0.37,0.23}% Diagram color red % 70%
\definecolor{fzjgreen}{RGB}{125,150,110}% Diagram color green % 100%
\definecolor{fzjlightgreen}{cmyk}{0.08,0.00,0.17,0.31}% Diagram color green % 70%
\definecolor{fzjyellow}{RGB}{215,170,80}% Diagram color yellow % 100%
\definecolor{fzjlightyellow}{cmyk}{0.00,0.11,0.29,0.07}% Diagram color yellow % 50%

\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=fzjbranchred}
\setbeamercolor{example text}{fg=fzjblue20}
\setbeamercolor{emphasized text}{fg=fzjblue20}

\setbeamercolor{structure}{fg=fzjblue50}
\setbeamercolor{fzjblue50}{bg=fzjblue50}
\setbeamercolor{fzjblue35}{bg=fzjblue35}
\setbeamercolor{fzjblue30}{bg=fzjblue30}
\setbeamercolor{fzjblue20}{bg=fzjblue20}
\setbeamercolor{fzjblue10}{bg=fzjblue10}

\setbeamercolor{fzjgray80}{bg=fzjgray80}
\setbeamercolor{fzjgray50}{bg=fzjgray50}
\setbeamercolor{fzjgray30}{bg=fzjgray30}

\setbeamercolor{fzjwhite}{bg=fzjwhite}

\setbeamercolor{fzjbranchred}{bg=fzjbranchred}
\setbeamercolor{fzjbranchyellow}{bg=fzjbranchyellow}
\setbeamercolor{fzjbranchblue}{bg=fzjbranchblue}

\setbeamercolor{fzjred}{bg=fzjred}
\setbeamercolor{fzjlightred}{bg=fzjlightred}
\setbeamercolor{fzjgreen}{bg=fzjgreen}
\setbeamercolor{fzjlightgreen}{bg=fzjlightgreen}
\setbeamercolor{fzjblue}{bg=fzjblue}
\setbeamercolor{fzjlightblue}{bg=fzjlightblue}
\setbeamercolor{fzjyellow}{bg=fzjyellow}
\setbeamercolor{fzjlightyellow}{bg=fzjlightyellow}

\setbeamercolor{title}{fg=white}
\setbeamercolor{date}{fg=white}
\setbeamercolor{author}{fg=white}
\setbeamercolor{date in head/foot}{fg=structure}

\setbeamercolor{part name}{fg=white}
\setbeamercolor{part title}{fg=white}

% Block stuff
\setbeamercolor{block title}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=fzjbranchred!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=fzjblue10!10!bg}

\mode<all>
